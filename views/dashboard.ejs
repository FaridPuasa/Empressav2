    <%- include('partials/logoutModal.ejs') %>

    <!-- scripts goes here -->
    <%- include('partials/scripts.ejs') %>

	<script>
		function update() {
			$('#clock').html(moment().format('dddd | Do MMMM YYYY | h:mm a'));
		}
	
		setInterval(update, 1000);
	</script>

<script>
    $(function() {
  var alert = $('div.alert[auto-close]');
  alert.each(function() {
    var that = $(this);
    var time_period = that.attr('auto-close');
    setTimeout(function() {
      that.alert('close');
    }, time_period);
  });
});
</script>

</body>
